{
  "version": 2,
  "builds": [
    {
      "src": "web/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/canvaskit/(.*)", "dest": "/canvaskit/$1" },
    { "src": "/icons/(.*)", "dest": "/icons/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|woff|woff2|ttf|eot))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
